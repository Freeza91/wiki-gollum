<main role="main">
  <section class="content">
    <p>共搜索到<%= @wikis.size %>条结果</p>
    <nav class="wiki-nav">
      <ul>
      <% @wikis.each do |wiki| %>
        <li>
          <%= link_to "#{ wiki.name }", wiki_path(wiki.name), 'data-no-turbolink' => false %>
        </li>
      <% end %>
      </ul>
    </nav>
  </section>
</main>
